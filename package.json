{
  "name": "remote-editor",
  "displayName": "remote-editor",
  "description": "Edit remote files as though they were local!",
  "version": "0.0.1",
  "publisher": "carlevans",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:remote.editor.connectRemote",
    "workspaceContains:.remote"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "remote.editor.connectRemote",
        "title": "Connect remote SSH filesystem"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "node-ssh": "^4.0.1"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "node-ssh": "^4.0.1",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
